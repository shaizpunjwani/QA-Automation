pipeline{
agent any
stages 
{
stage('Build') 
{
steps{
echo "Building the Project.........."
bat "mvn clean"
}
}
stage('Test') 
{
steps{
echo "Testing the Project.........."
bat "mvn test -Dsurefire.suiteXmlFiles=testng.xml"
}
}
stage('Compile')
{
steps{
echo "Compiling the Project........"
bat "mvn compile"
}
}
stage('Deploy') 
{
steps{
echo "Deploying the Project.........."
}
}
}
}